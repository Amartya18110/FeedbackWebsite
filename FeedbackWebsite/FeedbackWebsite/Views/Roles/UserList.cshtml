@model IEnumerable<FeedbackWebsite.Models.User>

<h2>List of users</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td class="text-right">
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access rights</a>
            </td>
        </tr>
    }
</table>
<a class="btn btn-secondary" asp-controller="Roles" asp-action="Index" role="button">Back to roles list</a>